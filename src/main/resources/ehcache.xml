<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
    <diskStore path="java.io.tmpdir"/>
    <cache name="plantUmlImages"
           maxElementsInMemory="200"
           maxElementsOnDisk="2000"
           eternal="false"
           overflowToDisk="true"
           diskPersistent="true"
           timeToIdleSeconds="120"
           timeToLiveSeconds="120" memoryStoreEvictionPolicy="LFU"/>
    <cache name="uniqueImages"
           maxElementsInMemory="800"
           maxElementsOnDisk="2000"
           eternal="false"
           overflowToDisk="true"
           diskPersistent="true"
           timeToIdleSeconds="12000"
           timeToLiveSeconds="12000" memoryStoreEvictionPolicy="LFU"/>
    <cache name="plantUmlFiles"
           maxElementsInMemory="800"
           maxElementsOnDisk="8000"
           eternal="false"
           overflowToDisk="true"
           diskPersistent="true"
           timeToIdleSeconds="12000"
           timeToLiveSeconds="12000" memoryStoreEvictionPolicy="LFU"/>
</ehcache>